doctype html
html(lang="ko")
  head
    include ../partials/headIncl
  body
    div.row 
      div.col-md-4.col-lg-3.col-xl-2.d-none.d-md-inline-block
        include ../partials/sideMenu
      div.coco-main-container.col-12.col-md-8.col-lg-9.col-xl-10
        main.coco-container
          block content
        include ../partials/footer
    include ../partials/cocoPlayer
    
    div.d-md-none
      include ../partials/mobileMenu

    //- hidden section
    div.hidden-section
      include ../partials/playList
      include ../partials/nowPlay
  script(src="/static/js/main.js") 
  script(src="/static/js/common.js") 
  block scripts